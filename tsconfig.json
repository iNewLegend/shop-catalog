{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "esnext",
		"target": "es5",
		"allowJs": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"@@": ["frontend/"],
		}
	},
	"include": [
		"frontend"
	],
	"exclude": [
		"node_modules"
	]
}
